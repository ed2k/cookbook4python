\par         \}
\par 
\par         /*  Table lookup. */
\par 
\par         heurTable.cho1[0][0][0]=UNCLEAR;
\par         heurTable.cho2[0][0][0]=LOSER;
\par         heurTable.cho1[0][0][1]=UNCLEAR;
\par         heurTable.cho2[0][0][1]=LOSER;
\par         heurTable.cho1[0][0][2]=UNCLEAR;
\par         heurTable.cho2[0][0][2]=LOSER;
\par         heurTable.cho1[0][0][3]=UNCLEAR;
\par         heurTable.cho2[0][0][3]=LOSER;
\par         heurTable.cho1[0][0][4]=LOSER;
\par         heurTable.cho2[0][0][4]=UNCLEAR;
\par         heurTable.cho1[0][0][5]=LOSER;
\par         heurTable.cho2[0][0][5]=UNCLEAR;
\par         heurTable.cho1[0][1][0]=UNCLEAR;
\par         heurTable.cho2[0][1][0]=LOSER;
\par         heurTable.cho1[0][1][2]=UNCLEAR;
\par         heurTable.cho2[0][1][2]=LOSER;
\par         heurTable.cho1[0][1][5]=LOSER;
